    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dp_12" />
            <solid android:color="@color/blue_4556b3" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_12" />
